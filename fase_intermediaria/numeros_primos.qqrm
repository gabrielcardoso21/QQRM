//20 primeiros numeros primos:
n = 20;
i = 1;
x = 2;
|( i < n ) {
    ?( x == 2) {
        !< x >;
    }
    r = x;
    x = x + 1;
    v = 0;
    |( r > 1 ) {
        ?( (x - ((x / r) * r)) == 0 ) {
            v = 1;
        }
        r = r - 1;
        ?( r == 1 ) {
            ?( v == 0 ) {
                !< x >;
                i = i + 1;
            }
        }
    }
}
